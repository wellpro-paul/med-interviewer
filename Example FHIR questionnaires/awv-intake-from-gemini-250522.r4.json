  {
    "resourceType": "Questionnaire",
    "id": "annual-wellness-visit-medical-history",
    "language": "en-US",
    "title": "Annual Wellness Visit - Medical History Questionnaire",
    "description": "A comprehensive medical history questionnaire for a primary care physician conducting an annual wellness visit.",
    "status": "draft",
    "date": "2025-05-21T23:11:00-04:00",
    "publisher": "Primary Care Clinic Example",
    "item": [
      {
        "linkId": "patient-information",
        "text": "I. Patient Information",
        "type": "group",
        "item": [
          {
            "linkId": "1.1",
            "text": "Full Name:",
            "type": "string",
            "required": true
          },
          {
            "linkId": "1.2",
            "code": [
              {
                "system": "http://loinc.org",
                "code": "21112-8",
                "display": "Birth date"
              }
            ],
            "text": "Date of Birth (MM/DD/YYYY):",
            "type": "date",
            "required": true
          },
          {
            "linkId": "1.3",
            "text": "Age:",
            "type": "integer"
          },
          {
            "linkId": "1.4",
            "code": [
              {
                "system": "http://loinc.org",
                "code": "76689-9",
                "display": "Sex assigned at birth"
              }
            ],
            "text": "Sex Assigned at Birth:",
            "type": "choice",
            "required": true,
            "answerOption": [
              {
                "valueCoding": {
                  "system": "http://hl7.org/fhir/administrative-gender",
                  "code": "male",
                  "display": "Male"
                }
              },
              {
                "valueCoding": {
                  "system": "http://hl7.org/fhir/administrative-gender",
                  "code": "female",
                  "display": "Female"
                }
              },
              {
                "valueString": "Other"
              }
            ]
          },
          {
              "linkId": "1.4.1",
              "text": "If Other, please specify:",
              "type": "string",
              "enableWhen": [
                  {
                      "question": "1.4",
                      "operator": "=",
                      "answerString": "Other"
                  }
              ],
              "enableBehavior": "all"
          },
          {
            "linkId": "1.5",
            "code": [
              {
                  "system": "http://loinc.org",
                  "code": "76691-5",
                  "display": "Gender identity"
              }
            ],
            "text": "Gender Identity (if different from sex assigned at birth):",
            "type": "string"
          },
          {
            "linkId": "1.6",
            "text": "Preferred Pronouns:",
            "type": "string"
          },
          {
            "linkId": "1.7",
            "text": "Address:",
            "type": "string"
          },
          {
            "linkId": "1.8",
            "text": "City:",
            "type": "string"
          },
          {
            "linkId": "1.9",
            "text": "State:",
            "type": "string"
          },
          {
            "linkId": "1.10",
            "text": "Zip Code:",
            "type": "string"
          },
          {
            "linkId": "1.11",
            "text": "Phone Number (Mobile):",
            "type": "string"
          },
          {
            "linkId": "1.12",
            "text": "Phone Number (Home):",
            "type": "string"
          },
          {
            "linkId": "1.13",
            "text": "Email Address:",
            "type": "string"
          },
          {
            "linkId": "1.14",
            "text": "Emergency Contact Name:",
            "type": "string"
          },
          {
            "linkId": "1.15",
            "text": "Emergency Contact Phone:",
            "type": "string"
          },
          {
            "linkId": "1.16",
            "text": "Emergency Contact Relationship:",
            "type": "string"
          }
        ]
      },
      {
        "linkId": "reason-for-visit",
        "text": "II. Reason for Visit / Health Goals",
        "type": "group",
        "item": [
          {
            "linkId": "2.1",
            "text": "What are your main health concerns today? (Even for a wellness visit, please list any specific questions or concerns you have)",
            "type": "text"
          },
          {
            "linkId": "2.2",
            "text": "What are your health goals for the next year? (e.g., lose weight, exercise more, manage stress)",
            "type": "text"
          }
        ]
      },
      {
        "linkId": "past-medical-history",
        "text": "III. Past Medical History",
        "type": "group",
        "item": [
          {
            "linkId": "3.1",
            "text": "Have you ever been diagnosed with any of the following conditions? (Check all that apply)",
            "type": "group",
            "repeats": false,
            "item": [
              {"linkId": "3.1.1", "text": "High Blood Pressure", "type": "boolean"},
              {"linkId": "3.1.2", "text": "High Cholesterol", "type": "boolean"},
              {"linkId": "3.1.3", "text": "Diabetes (Type 1 or 2)", "type": "boolean"},
              {"linkId": "3.1.4", "text": "Prediabetes", "type": "boolean"},
              {"linkId": "3.1.5", "text": "Heart Attack", "type": "boolean"},
              {"linkId": "3.1.6", "text": "Stroke", "type": "boolean"},
              {"linkId": "3.1.7", "text": "Atrial Fibrillation", "type": "boolean"},
              {"linkId": "3.1.8", "text": "Congestive Heart Failure", "type": "boolean"},
              {"linkId": "3.1.9", "text": "Asthma", "type": "boolean"},
              {"linkId": "3.1.10", "text": "COPD / Emphysema", "type": "boolean"},
              {"linkId": "3.1.11", "text": "Sleep Apnea", "type": "boolean"},
              {
                "linkId": "3.1.12",
                "text": "Cancer",
                "type": "boolean",
                "item": [
                  {
                    "linkId": "3.1.12.1",
                    "text": "Type:",
                    "type": "string",
                    "enableWhen": [{"question": "3.1.12", "operator": "=", "answerBoolean": true}],
                    "enableBehavior": "all"
                  },
                  {
                    "linkId": "3.1.12.2",
                    "text": "Year Diagnosed:",
                    "type": "integer",
                    "enableWhen": [{"question": "3.1.12", "operator": "=", "answerBoolean": true}],
                    "enableBehavior": "all"
                  }
                ]
              },
              {"linkId": "3.1.13", "text": "Thyroid Problems (Hyper/Hypo)", "type": "boolean"},
              {"linkId": "3.1.14", "text": "Kidney Disease", "type": "boolean"},
              {"linkId": "3.1.15", "text": "Liver Disease (e.g., Hepatitis, Cirrhosis)", "type": "boolean"},
              {"linkId": "3.1.16", "text": "Arthritis (Osteoarthritis/Rheumatoid)", "type": "boolean"},
              {"linkId": "3.1.17", "text": "Osteoporosis / Osteopenia", "type": "boolean"},
              {"linkId": "3.1.18", "text": "GERD / Acid Reflux", "type": "boolean"},
              {"linkId": "3.1.19", "text": "Irritable Bowel Syndrome (IBS)", "type": "boolean"},
              {"linkId": "3.1.20", "text": "Inflammatory Bowel Disease (Crohn's/Colitis)", "type": "boolean"},
              {"linkId": "3.1.21", "text": "Anemia", "type": "boolean"},
              {"linkId": "3.1.22", "text": "Blood Clots (DVT/PE)", "type": "boolean"},
              {"linkId": "3.1.23", "text": "Migraines / Frequent Headaches", "type": "boolean"},
              {"linkId": "3.1.24", "text": "Depression", "type": "boolean"},
              {"linkId": "3.1.25", "text": "Anxiety", "type": "boolean"}
            ]
          },
          {
            "linkId": "3.2",
            "text": "Other significant medical conditions:",
            "type": "text"
          },
          {
            "linkId": "3.3",
            "text": "Surgeries: Please list any surgeries you have had, including the approximate year.",
            "type": "group",
            "repeats": true,
            "item": [
              {"linkId": "3.3.1", "text": "Surgery Type", "type": "string"},
              {"linkId": "3.3.2", "text": "Year", "type": "integer"},
              {"linkId": "3.3.3", "text": "Hospital/Surgeon (Optional)", "type": "string"}
            ]
          },
          {
            "linkId": "3.4",
            "text": "Hospitalizations: Please list any hospitalizations (other than for surgeries listed above), including reason and approximate year.",
            "type": "group",
            "repeats": true,
            "item": [
              {"linkId": "3.4.1", "text": "Reason for Hospitalization", "type": "string"},
              {"linkId": "3.4.2", "text": "Year", "type": "integer"},
              {"linkId": "3.4.3", "text": "Hospital (Optional)", "type": "string"}
            ]
          }
        ]
      },
      {
        "linkId": "medications",
        "text": "IV. Medications",
        "type": "group",
        "item": [
          {
            "linkId": "4.1",
            "text": "Please list ALL medications you are currently taking, including prescriptions, over-the-counter drugs, vitamins, and herbal supplements.",
            "type": "group",
            "repeats": true,
            "item": [
              {"linkId": "4.1.1", "text": "Medication Name", "type": "string", "required": true},
              {"linkId": "4.1.2", "text": "Dose (e.g., 50mg)", "type": "string"},
              {"linkId": "4.1.3", "text": "Frequency (e.g., twice daily)", "type": "string"},
              {"linkId": "4.1.4", "text": "Reason for Taking", "type": "string"},
              {"linkId": "4.1.5", "text": "Prescribing Doctor (if known)", "type": "string"}
            ]
          },
          {
            "linkId": "4.2",
            "text": "Do you use a mail-order pharmacy?",
            "type": "boolean"
          },
          {
            "linkId": "4.3",
            "text": "Pharmacy Name & Phone:",
            "type": "string"
          }
        ]
      },
      {
        "linkId": "allergies",
        "text": "V. Allergies",
        "type": "group",
        "item": [
          {
            "linkId": "5.1",
            "text": "Are you allergic to any medications, foods, or other substances (e.g., latex, bee stings)?",
            "type": "boolean",
            "required": true
          },
          {
            "linkId": "5.2",
            "text": "If yes, please list them and describe the reaction:",
            "type": "group",
            "repeats": true,
            "enableWhen": [
              {
                "question": "5.1",
                "operator": "=",
                "answerBoolean": true
              }
            ],
            "enableBehavior": "all",
            "item": [
              {"linkId": "5.2.1", "text": "Allergen", "type": "string", "required": true},
              {"linkId": "5.2.2", "text": "Reaction Described", "type": "string", "required": true}
            ]
          }
        ]
      },
      {
        "linkId": "family-history",
        "text": "VI. Family History",
        "type": "group",
        "item": [
          {
            "linkId": "6.1",
            "text": "Has anyone in your immediate family (parents, siblings, children) had any of the following conditions?",
            "type": "group",
            "item": [
              {
                "linkId": "6.1.1", "text": "Heart Disease", "type": "group",
                "item": [
                  {"linkId": "6.1.1.1", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.1.2", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              },
              {
                "linkId": "6.1.2", "text": "Stroke", "type": "group",
                "item": [
                  {"linkId": "6.1.2.1", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.2.2", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              },
              {
                "linkId": "6.1.3", "text": "High Blood Pressure", "type": "group",
                "item": [
                  {"linkId": "6.1.3.1", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.3.2", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              },
              {
                "linkId": "6.1.4", "text": "High Cholesterol", "type": "group",
                "item": [
                  {"linkId": "6.1.4.1", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.4.2", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              },
              {
                "linkId": "6.1.5", "text": "Diabetes", "type": "group",
                "item": [
                  {"linkId": "6.1.5.1", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.5.2", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              },
              {
                "linkId": "6.1.6", "text": "Cancer", "type": "group", "repeats": true,
                "item": [
                  {"linkId": "6.1.6.1", "text": "Cancer Type", "type": "string"},
                  {"linkId": "6.1.6.2", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.6.3", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              },
              {
                "linkId": "6.1.7", "text": "Asthma", "type": "group",
                "item": [
                  {"linkId": "6.1.7.1", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.7.2", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              },
              {
                "linkId": "6.1.8", "text": "Thyroid Disease", "type": "group",
                "item": [
                  {"linkId": "6.1.8.1", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.8.2", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              },
              {
                "linkId": "6.1.9", "text": "Osteoporosis", "type": "group",
                "item": [
                  {"linkId": "6.1.9.1", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.9.2", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              },
              {
                "linkId": "6.1.10", "text": "Dementia / Alzheimer's", "type": "group",
                "item": [
                  {"linkId": "6.1.10.1", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.10.2", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              },
              {
                "linkId": "6.1.11", "text": "Mental Health Conditions (e.g., Depression, Anxiety, Bipolar)", "type": "group",
                "item": [
                  {"linkId": "6.1.11.1", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.11.2", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              },
              {
                "linkId": "6.1.12", "text": "Substance Abuse", "type": "group",
                "item": [
                  {"linkId": "6.1.12.1", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.12.2", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              },
              {
                "linkId": "6.1.13", "text": "Other Genetic Conditions", "type": "group",
                "item": [
                  {"linkId": "6.1.13.1", "text": "Specify condition", "type": "string"},
                  {"linkId": "6.1.13.2", "text": "Relationship(s)", "type": "string"},
                  {"linkId": "6.1.13.3", "text": "Age at Diagnosis (Approx.)", "type": "integer"}
                ]
              }
            ]
          }
        ]
      },
      {
        "linkId": "social-history",
        "text": "VII. Social History",
        "type": "group",
        "item": [
          {"linkId": "7.1", "text": "Occupation:", "type": "string"},
          {"linkId": "7.2", "text": "Highest Level of Education Completed:", "type": "string"},
          {
            "linkId": "7.3", "text": "Marital Status:", "type": "choice",
            "answerOption": [
              {"valueString": "Single"}, {"valueString": "Married"}, {"valueString": "Partnered"},
              {"valueString": "Divorced"}, {"valueString": "Widowed"}
            ]
          },
          {
            "linkId": "7.4", "text": "Living Situation:", "type": "open-choice",
            "answerOption": [
              {"valueString": "Live Alone"}, {"valueString": "Live with Spouse/Partner"}, {"valueString": "Live with Family"}
            ]
          },
          {"linkId": "7.5", "text": "Do you feel safe at home?", "type": "boolean"},
          {
            "linkId": "7.6", "text": "Tobacco Use", "type": "group",
            "item": [
              {
                "linkId": "7.6.1",
                "text": "Have you ever smoked cigarettes, cigars, pipes, or used smokeless tobacco?",
                "type": "boolean"
              },
              {
                "linkId": "7.6.1.1", "text": "If yes, what type?", "type": "string",
                "enableWhen": [{"question": "7.6.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              },
              {
                "linkId": "7.6.1.2", "text": "How much per day?", "type": "string",
                "enableWhen": [{"question": "7.6.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              },
              {
                "linkId": "7.6.1.3", "text": "For how many years?", "type": "integer",
                "enableWhen": [{"question": "7.6.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              },
              {
                "linkId": "7.6.1.4", "text": "If you currently smoke, are you interested in quitting?", "type": "choice",
                "enableWhen": [{"question": "7.6.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all",
                "answerOption": [{"valueString": "Yes"}, {"valueString": "No"}, {"valueString": "Maybe"}]
              },
              {
                "linkId": "7.6.1.5", "text": "If you quit, when did you quit? (Year)", "type": "integer",
                "enableWhen": [{"question": "7.6.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              },
              {
                "linkId": "7.6.2", "text": "Do you use e-cigarettes or vape?", "type": "boolean"
              },
              {
                "linkId": "7.6.2.1", "text": "If yes, what product/flavor?", "type": "string",
                "enableWhen": [{"question": "7.6.2", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              },
              {
                "linkId": "7.6.2.2", "text": "How often?", "type": "string",
                "enableWhen": [{"question": "7.6.2", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              }
            ]
          },
          {
            "linkId": "7.7", "text": "Alcohol Use", "type": "group",
            "item": [
              {"linkId": "7.7.1", "text": "Do you drink alcohol?", "type": "boolean"},
              {
                "linkId": "7.7.1.1", "text": "If yes, how many days per week do you typically drink?", "type": "integer",
                "enableWhen": [{"question": "7.7.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              },
              {
                "linkId": "7.7.1.2", "text": "On a typical day when you drink, how many standard drinks do you have? (A standard drink is 12 oz beer, 5 oz wine, or 1.5 oz distilled spirits)", "type": "integer",
                "enableWhen": [{"question": "7.7.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              },
              {
                "linkId": "7.7.1.3", "text": "Have you ever felt you needed to cut down on your drinking?", "type": "boolean",
                "enableWhen": [{"question": "7.7.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              },
              {
                "linkId": "7.7.1.4", "text": "In the past year, have you had more than 4 drinks (female) or 5 drinks (male) on one occasion?", "type": "boolean",
                "enableWhen": [{"question": "7.7.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              }
            ]
          },
          {
            "linkId": "7.8", "text": "Substance Use", "type": "group",
            "item": [
              {
                "linkId": "7.8.1",
                "text": "Do you currently use or have you in the past used recreational drugs or substances (e.g., marijuana, cocaine, opioids, methamphetamines, etc.)?",
                "type": "boolean"
              },
              {
                "linkId": "7.8.1.1", "text": "If yes, please specify type and frequency (current or past):", "type": "text",
                "enableWhen": [{"question": "7.8.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              },
              {"linkId": "7.8.2", "text": "Do you misuse prescription medications?", "type": "boolean"}
            ]
          },
          {
            "linkId": "7.9", "text": "Diet & Nutrition", "type": "group",
            "item": [
              {"linkId": "7.9.1", "text": "How would you describe your diet? (e.g., balanced, vegetarian, vegan, low-salt, low-fat)", "type": "string"},
              {"linkId": "7.9.2", "text": "How many servings of fruits and vegetables do you eat per day on average?", "type": "integer"},
              {"linkId": "7.9.3", "text": "How many meals do you typically eat per day?", "type": "integer"},
              {
                "linkId": "7.9.4", "text": "Do you have any dietary restrictions or food intolerances?", "type": "boolean",
                "item": [
                  {
                    "linkId": "7.9.4.1", "text": "If yes:", "type": "string",
                    "enableWhen": [{"question": "7.9.4", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
                  }
                ]
              },
              {
                "linkId": "7.9.5", "text": "Are you concerned about your weight?", "type": "boolean",
                "item": [
                  {
                    "linkId": "7.9.5.1", "text": "If yes:", "type": "choice",
                    "enableWhen": [{"question": "7.9.5", "operator": "=", "answerBoolean": true}], "enableBehavior": "all",
                    "answerOption": [{"valueString": "Want to lose"}, {"valueString": "Want to gain"}]
                  }
                ]
              },
              {"linkId": "7.9.6", "text": "How much caffeine do you consume daily (coffee, tea, soda, energy drinks)?", "type": "string"}
            ]
          },
          {
            "linkId": "7.10", "text": "Exercise & Physical Activity", "type": "group",
            "item": [
              {"linkId": "7.10.1", "text": "Do you exercise regularly?", "type": "boolean"},
              {
                "linkId": "7.10.1.1", "text": "If yes, what type of exercise?", "type": "string",
                "enableWhen": [{"question": "7.10.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              },
              {
                "linkId": "7.10.1.2", "text": "How many days per week?", "type": "integer",
                "enableWhen": [{"question": "7.10.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              },
              {
                "linkId": "7.10.1.3", "text": "For how long each session? (minutes)", "type": "integer",
                "enableWhen": [{"question": "7.10.1", "operator": "=", "answerBoolean": true}], "enableBehavior": "all"
              },
              {
                "linkId": "7.10.2", "text": "How would you describe your usual level of physical activity outside of formal exercise?", "type": "choice",
                "answerOption": [
                  {"valueString": "Sedentary (mostly sitting)"}, {"valueString": "Light (some walking)"},
                  {"valueString": "Moderate (on feet often)"}, {"valueString": "Vigorous (physically demanding work/hobbies)"}
                ]
              }
            ]
          },
          {
              "linkId": "7.11", "text": "Sleep", "type": "group",
              "item": [
                  {"linkId": "7.11.1", "text": "On average, how many hours of sleep do you get per night?", "type": "decimal"},
                  {"linkId": "7.11.2", "text": "Do you have trouble falling asleep or staying asleep?", "type": "boolean"},
                  {"linkId": "7.11.3", "text": "Do you snore loudly?", "type": "boolean"},
                  {"linkId": "7.11.4", "text": "Has anyone told you that you stop breathing during sleep?", "type": "boolean"},
                  {"linkId": "7.11.5", "text": "Do you generally feel rested upon waking?", "type": "boolean"}
              ]
          },
          {
              "linkId": "7.12", "text": "Stress", "type": "group",
              "item": [
                  {
